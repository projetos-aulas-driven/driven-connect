generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model contacts {
  id       Int      @id @default(autoincrement())
  fullname String   @db.VarChar(255)
  email    String?  @db.VarChar(255)
  picture  String?  @db.VarChar(255)
  phones   phones[]
}

model phones {
  id         Int      @id @default(autoincrement())
  number     String   @db.VarChar(20)
  contact_id Int
  contacts   contacts @relation(fields: [contact_id], references: [id], onDelete: Cascade, map: "fk_contato_id")
}
